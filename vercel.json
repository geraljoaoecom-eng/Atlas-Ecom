{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/index.js"
    },
    {
      "source": "/dashboard",
      "destination": "/index.js"
    },
    {
      "source": "/login",
      "destination": "/index.js"
    },
    {
      "source": "/",
      "destination": "/index.js"
    }
  ],
  "functions": {
    "index.js": {
      "includeFiles": "public/**"
    }
  }
}
