{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run playwright:install"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/client.js",
      "destination": "/index.js"
    },
    {
      "source": "/index.html",
      "destination": "/index.js"
    },
    {
      "source": "/login.html",
      "destination": "/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/index.js"
    },
    {
      "source": "/dashboard",
      "destination": "/index.js"
    },
    {
      "source": "/login",
      "destination": "/index.js"
    },
    {
      "source": "/",
      "destination": "/index.js"
    }
  ]
}
